<dashboard script="credential_handler.js" version="1.1">
  <label>Configuration Dashboard</label>
  <row>
    <panel>
      <html>
        <h1> Form </h1>
        <form id="collect">
          <h2> Server 1 </h2>
          <label for="url">Server 1 URL:</label>
          <input type="text" token="url" id="url"/>
          <label for="username">Server 1 User Name:</label>
          <input type="text" token="username" id="username"/>
          <label for="password">Server 1 Password:</label>
          <input type="password" token="password" id="password"/>
        </form>
        <div>
          <button class="btn" id="submit-config">Submit Configuration</button>
          <div id="response-msg"></div>
        </div>
      </html>
    </panel>
  </row>
</dashboard>